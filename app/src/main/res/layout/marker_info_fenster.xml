<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@android:color/white">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textViewMarkerInfoFensterHeader"
        android:text="header"
        android:textColor="#000000"
        android:layout_gravity="center_horizontal"
        android:textStyle="bold"
        android:layout_marginTop="5pt"
        android:layout_marginBottom="5pt"/>
    <View
        android:layout_width="fill_parent"
        android:layout_height="1dip"
        android:background="#000000" />


    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:id="@+id/markerInfoFensterPunktInfoLinearLayout"
        android:layout_gravity="center_horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Projektnummer"
            android:textColor="#000000"
            android:layout_marginTop="5pt"
            android:id="@+id/textViewMarkerInfoFensterProjektNummer"
            android:layout_gravity="center_horizontal"/>

        <View
            android:layout_width="fill_parent"
            android:layout_height="1dip"
            android:background="#000000"
            android:layout_marginLeft="10pt"
            android:layout_marginRight="10pt"
            android:id="@+id/viewMarker1"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Rechtswert"
            android:textColor="#000000"
            android:id="@+id/textViewMarkerInfoFensterRechtsWert"
            android:layout_gravity="center_horizontal"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Hochwert"
            android:textColor="#000000"
            android:id="@+id/textViewMarkerInfoFensterHochWert"
            android:layout_gravity="center_horizontal"/>

        <View
            android:layout_width="fill_parent"
            android:layout_height="1dip"
            android:background="#000000"
            android:layout_marginLeft="10pt"
            android:layout_marginRight="10pt"
            android:id="@+id/viewMarker2"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Hoehe"
            android:textColor="#000000"
            android:id="@+id/textViewMarkerInfoFensterHoehe"
            android:layout_gravity="center_horizontal"/>
        <View
            android:layout_width="fill_parent"
            android:layout_height="1dip"
            android:background="#000000"
            android:layout_marginLeft="10pt"
            android:layout_marginRight="10pt" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Latitude"
            android:textColor="#000000"
            android:id="@+id/textViewMarkerInfoFensterLatitude"
            android:layout_gravity="center_horizontal"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Longitude"
            android:textColor="#000000"
            android:id="@+id/textViewMarkerInfoFensterLongitude"
            android:layout_gravity="center_horizontal"/>

        <View
            android:layout_width="fill_parent"
            android:layout_height="1dip"
            android:background="#000000"
            android:layout_marginLeft="10pt"
            android:layout_marginRight="10pt" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Punktart"
            android:textColor="#000000"
            android:id="@+id/textViewMarkerInfoFensterPunktArt"
            android:layout_gravity="center_horizontal"/>
        <View
            android:layout_width="fill_parent"
            android:layout_height="1dip"
            android:background="#000000"
            android:layout_marginLeft="10pt"
            android:layout_marginRight="10pt"
            android:layout_marginBottom="5pt"
            android:id="@+id/viewMarker3"/>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_gravity="center_horizontal"
            android:id="@+id/markerInfoFensterBilderLinearLayout">

        </LinearLayout>
    </LinearLayout>


</LinearLayout>